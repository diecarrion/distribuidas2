package entities;

import java.util.Date;

public class RemitoProveedor {
	private int id;
	private Date fecha;
	private Proveedor proveedor;
	private OrdenCompraProveedor ordenProveedor;
	
	public RemitoCliente() {

	}

	public RemitoCliente(int id, Date fecha){
		this.id = id;
		this.fecha = fecha;
	}
}
