package entities;

import java.util.Date;

public class BultoCC {
	private int id;
	private Date fecha;
	private RemitoTransporte remito;
	
	public BultoCC() {

	}

	public BultoCC(int id, Date fecha){
		this.id = id;
		this.fecha = fecha;
	}
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	public Date getFecha() {
		return fecha;
	}

	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}
}
