package entities;

import java.util.Date;

public class BultoOV {
	private int id;
	private Date fecha;
	//private OficinaVenta;
	private Factura factura;
	private Cliente cliente;
	private RemitoCliente remito;
	
	public BultoOV() {

	}

	public BultoOV(int id, Date fecha){
		this.id = id;
		this.fecha = fecha;
	}
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	public Date getFecha() {
		return fecha;
	}

	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}
}
