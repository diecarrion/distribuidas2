package entities;

import java.util.Date;

public class Pedido {
	
	private int id;
	private Date fecha;
	private OficinaVenta oficina;
	private Cliente cliente;
	private Cotizacion cotizacion;
	
	public Pedido() {

	}
	
	public Pedido(int id, Date fecha, OficinaVenta oficina, Cliente cliete, Cotizacion cotizacion){
		this.id = id;
		this.fecha = fecha;
	}
	

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	public Date getFecha() {
		return fecha;
	}

	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}
}
