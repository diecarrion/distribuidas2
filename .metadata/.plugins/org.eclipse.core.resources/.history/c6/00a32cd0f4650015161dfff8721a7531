package entities;

import java.util.Date;

public class Pedido {
	
	private int id;
	private Date fecha;
	//private OficinaVentas oficina;
	private Cliente cliente;
	private Cotizacion cotizacion;
	
	public Pedido() {

	}
	
	public Pedido(int id, Date fecha){
		this.id = id;
		this.fecha = fecha;
	}
}
