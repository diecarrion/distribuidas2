package dto;

import java.io.Serializable;

public class ItemRemitoProveedorVO implements Serializable {

	private int id;
	private int cantidad;
	private RodamientoVO rodamiento;
	
	
	public ItemRemitoProveedorVO(int id, int cantidad,
			RodamientoVO rodamiento) {
		super();
		this.id = id;
		this.cantidad = cantidad;
		this.rodamiento = rodamiento;
	}
	public ItemRemitoProveedorVO() {
		// TODO Auto-generated constructor stub
	}
	/**
	 * @return the idItemRemito
	 */
	public int getId() {
		return id;
	}
	/**
	 * @param idItemRemito the idItemRemito to set
	 */
	public void setIdItemRemito(int idItemRemito) {
		this.idItemRemito = idItemRemito;
	}
	/**
	 * @return the cantidad
	 */
	public int getCantidad() {
		return cantidad;
	}
	/**
	 * @param cantidad the cantidad to set
	 */
	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}
	/**
	 * @return the rodamiento
	 */
	public RodamientoView getRodamiento() {
		return rodamiento;
	}
	/**
	 * @param rodamiento the rodamiento to set
	 */
	public void setRodamiento(RodamientoView rodamiento) {
		this.rodamiento = rodamiento;
	}
	
}
